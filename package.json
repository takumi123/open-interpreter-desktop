{
	"name": "tangent",
	"version": "0.0.1",
	"main": "src/electron.cjs",
	"author": "Ty Fiero",
	"description": "A desktop app for conversation",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#000',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "vite dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "vite build",
		"build-both": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"build:svelte": "vite build",
		"build:electron": "electron-builder -mwl --config build.config.json"
	},
	"browserslist": [
		"Chrome 89"
	],
	"engines": {
		"npm": ">=7"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.5.0",
		"@types/uuid": "^9.0.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@zerodevx/svelte-toast": "^0.9.5",
		"autoprefixer": "^10.4.15",
		"electron": "^26.2.0",
		"electron-builder": "^22.10.5",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.28",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0",
		"vitest": "^0.25.3"
	},
	"optionalDependencies": {
		"dmg-license": "~1.0.9"
	},
	"dependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@tailwindcss/typography": "^0.5.10",
		"ai": "^2.2.10",
		"busboy": "^1.6.0",
		"concurrently": "^8.2.1",
		"cross-env": "^7.0.3",
		"cross-fetch": "^4.0.0",
		"dotenv": "^16.3.1",
		"electron-connect": "^0.6.3",
		"electron-context-menu": "^3.6.1",
		"electron-packager": "^17.1.2",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-updater": "^4.3.9",
		"electron-window-state": "^5.0.3",
		"flowbite": "^1.6.3",
		"flowbite-svelte": "^0.29.13",
		"formidable": "^3.5.1",
		"highlight.js": "^11.8.0",
		"marked": "^9.0.0",
		"npm": "^10.1.0",
		"openai": "^4.6.0",
		"svelte-legos": "^0.2.1",
		"svelte-modals": "^1.3.0",
		"uuid": "^9.0.1",
		"whisper-node": "^0.2.12",
		"whisper-nodejs": "^1.0.0"
	},
	"overrides": {
		"electron": {
			"got": "^12.5.1"
		}
	}
}
